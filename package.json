{
  "name": "@totalpave/hashmap",
  "version": "1.0.2-dev",
  "description": "A generic hashmap collection class",
  "main": "lib/api.js",
  "types": "lib/api.d.ts",
  "scripts": {
    "build": "npm run-script lint && tsc",
    "lint": "node --max_old_space_size=6144 ./node_modules/eslint/bin/eslint.js --ext .ts '?(src|spec)/**/*.ts' --cache",
    "unit": "ts-node ./spec/support/jasmine.ts",
    "test": "npm run-script lint && nyc --reporter=lcov --reporter=text ts-node ./spec/support/jasmine.ts",
    "prepublishOnly": "npm run-script build && npm run build-docs",
    "build-docs": "typedoc --excludePrivate --excludeNotExported --tsBuildInfoFile ./docs/.tsbuildinfo --mode file -readme ./README.md --out ./docs ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/totalpave/hashmap.git"
  },
  "keywords": [
    "dictionary",
    "dict",
    "hash",
    "map",
    "hashmap",
    "totalpave",
    "total",
    "pave"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "author": "norman.breau@totalpave.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/totalpave/hashmap/issues"
  },
  "homepage": "https://github.com/totalpave/hashmap#readme",
  "devDependencies": {
    "@totalpave/eslint-plugin": "2.3.2",
    "@types/jasmine": "3.4.5",
    "@typescript-eslint/eslint-plugin": "2.6.1",
    "@typescript-eslint/parser": "2.6.1",
    "eslint": "6.6.0",
    "jasmine": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "nyc": "14.1.1",
    "ts-node": "8.4.1",
    "tslib": "1.10.0",
    "tsutils": "3.17.1",
    "typedoc": "0.15.0",
    "typescript": "3.6.4"
  },
  "dependencies": {
    "@types/node": "12.12.5"
  }
}
